generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measure {
  measure_uuid     String   @id
  measure_datetime DateTime
  measure_type     String
  has_confirmed    Boolean  @default(false)
  image_url        String
  customer_code    String
}
